{
    "name": "wirecard/extentions-credentials-module",
    "description": "Module used in shop extentions to read/load default credentials",
    "require": {
        "php": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^8"
    },
    "scripts": {
        "cs-check": "./vendor/bin/phpcs ./src",
        "cs-fix": "./vendor/bin/phpcbf ./src",
        "test": "./vendor/bin/phpunit ./src"
    },
    "autoload": {
        "psr-4": {
            "ArtifactCreation\\": "src/"
        }
    }
}
