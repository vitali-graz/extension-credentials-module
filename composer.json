{
    "name": "wirecard/extentions-credentials-module",
    "description": "Module used in shop extentions to read/load default credentials",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "scripts": {
        "cs-check-configure": "/tmp/phpcs/bin/phpcs --config-set installed_paths /tmp/sniffs",
        "cs-check": "/tmp/phpcs/bin/phpcs --standard=./phpcs.xml .",
        "cs-fix": "phpcbf --standard=./phpcs.xml .",
        "test": "phpunit --colors"
    },
    "autoload": {
        "psr-4": {
            "ArtifactCreation\\": "src/"
        }
    }
}
